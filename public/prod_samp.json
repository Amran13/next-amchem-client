        ],
        "packSize": "25 kg bags",
        "description": "Light brown to yellowish-brown homogeneous powder with characteristic odor.",
        "pharmacologicalProperties": {
            "mechanismOfAction": "Flavophospholipol inhibits bacterial cell wall synthesis...",
            "spectrum": "Active against gram-positive organisms...",
            "pharmacokinetics": "Not absorbed from GI tract...",
            "scientificEvidence": "Studies in mSystems (2021) show microbial shifts..."
        },
        "indications": [
            "Growth promotion in poultry, swine, and cattle",
            "Prevention of necrotic enteritis",
            "Improved feed conversion ratio",
            "Reduction of pathogenic colonization",
            "Reduced antimicrobial resistance transfer"
        ],
        "dosageAndAdministration": {
            "poultry": {
                "broilers": "0.5–2.0 g/kg feed",
                "layers": "0.5–2.0 g/kg feed",
                "turkeys": "0.5–2.0 g/kg feed",
                "general": "Continuous administration"
            },
            "swine": {
                "starter": "1.0–2.0 g/kg feed",
                "growerFinisher": "0.5–1.0 g/kg feed",
                "sows": "0.5–1.0 g/kg feed",
                "general": ""
            },
            "cattle": {
                "feedlot": "0.5–1.5 g/kg feed",
                "general": ""
            },
            "therapeuticUse": {
                "swineDysentery": "",
                "proliferativeEnteropathy": "",
                "other": ""
            }
        },
        "contraindications": "Hypersensitivity to flavophospholipol; severe hepatic/renal dysfunction.",
        "withdrawalPeriod": {
            "poultryMeat": "0 days",
            "swineMeat": "0 days",
            "cattleMeat": "0 days",
            "eggs": "Not permitted in some jurisdictions"
        },
        "adverseEffects": "None reported.",
        "precautions": "Use at approved dose rates.",
        "drugInteractions": "No cross-resistance with β-lactams.",
        "storage": "Store at 5–25°C, protected from moisture and light.",
        "shelfLife": "24 months",
